import platform
import time

def work(size):
    start = time.time() * 1000
    junk = 5
    for number in range(size * 20000):
        junk = junk + number
    end = time.time() * 1000
    return round(10 * (end - start)) / 10

def main(params):
    size = params.get("size", 1)
    elapsed = work(size)
    result = "Size = " + str(size) + " Elapsed = " + str(elapsed) + " ms"
    return {"result": result }
